
/* ----------------------------------------*\
  CONTAINER
\* ---------------------------------------- */

.container {
  @apply mx-auto px-4 xs:px-7 sm:px-9 lg:px-10 xl:px-12 2xl:px-16;
  max-width: 1620px;
}

.container--flex {
  @apply flex justify-between;
}

.container--sm {
  max-width: 640px;
}

.container--md {
  max-width: 910px;
}
